<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/startEquilActivationFragment">
    <fragment
        android:id="@+id/startEquilActivationFragment"
        android:name="com.microtechmd.equil.ui.pair.EquilPairAssembleFragment"
        android:label="StartEquilActivationFragment">
        <action
            android:id="@+id/action_startEquilActivationFragment_to_startEquilSerialNumberFragment"
            app:destination="@id/startEquilSerialNumberFragment" />
    </fragment>
    <fragment
        android:id="@+id/startEquilSerialNumberFragment"
        android:name="com.microtechmd.equil.ui.pair.EquilPairSerialNumberFragment"
        android:label="StartEquilSerialNumberFragment">

        <action
            android:id="@+id/action_startEquilActivationFragment_to_startEquilPairFillFragment"
            app:destination="@id/startEquilPairFillFragment" />
    </fragment>

    <fragment
        android:id="@+id/startEquilPairFillFragment"
        android:name="com.microtechmd.equil.ui.pair.EquilPairFillFragment"
        android:label="StartEquilPairFillFragment">
        <action
            android:id="@+id/action_startEquilActivationFragment_to_startEquilPairConfirmFragment"
            app:destination="@id/startEquilPairConfirmFragment" />
    </fragment>
    <fragment
        android:id="@+id/startEquilPairConfirmFragment"
        android:name="com.microtechmd.equil.ui.pair.EquilPairConfirmFragment"
        android:label="StartEquilPairConfirmFragment"></fragment>
</navigation>